generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  name String @db.VarChar(200)
  role Int @default(1)
  email    String @db.VarChar(200)
  password String @db.VarChar(200)

  @@map("users")
}
